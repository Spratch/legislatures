          "portable_text_en": [
            {
              "_key": "d18de0bc9af9",
              "children": [
                {
                  "_type": "span",
                  "marks": [],
                  "text": "I also thank all the people who provided me with data, explanations, feedback and support.",
                  "_key": "93c2343b0f8d"
                }
              ],
              "markDefs": [],
              "_type": "block",
              "style": "normal"
            },
            {
              "_key": "e40eb7f9d15b",
              "children": [
                {
                  "_type": "span",
                  "marks": ["18d49ba42aa3"],
                  "text": "Content licensed under CC-BY-SA 4.0",
                  "_key": "6d583e394a73"
                },
                {
                  "_type": "span",
                  "marks": [],
                  "text": ". Source : Article ",
                  "_key": "34ae3e9ae2d4"
                },
                {
                  "_type": "span",
                  "marks": ["em", "47d441a5f282"],
                  "text": "Liste des législatures françaises",
                  "_key": "0e89cd653de6"
                },
