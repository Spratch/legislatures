          "portable_text_de": [
            {
              "_key": "875ec4544af7",
              "children": [
                {
                  "_type": "span",
                  "marks": [],
                  "text": "Außerdem danke ich allen Menschen, die mir Daten, Erklärungen, Feedback und Unterstützung zur Verfügung gestellt haben.",
                  "_key": "ffcd9d148386"
                }
              ],
              "markDefs": [],
              "_type": "block",
              "style": "normal"
            },
            {
              "_key": "0db74af3effd",
              "children": [
                {
                  "_type": "span",
                  "marks": ["42b900671733"],
                  "text": "Inhalt lizenziert unter CC-BY-SA 4.0",
                  "_key": "748571ff91c6"
                },
                {
                  "_type": "span",
                  "marks": [],
                  "text": ". Quelle: Artikel ",
                  "_key": "67732e4588de"
                },
                {
                  "_type": "span",
